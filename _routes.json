{
  "version": 1,
  "include": [
    {
      "pattern": "/config",
      "path": "server.js"
    },
    {
      "pattern": "/images",
      "path": "server.js"
    },
    {
      "pattern": "/exif/*",
      "path": "server.js"
    },
    {
      "pattern": "/thumbnail/*",
      "path": "server.js"
    },
    {
      "pattern": "/assets/*",
      "path": "public/$1"
    },
    {
      "pattern": "/styles/*",
      "path": "public/$1"
    },
    {
      "pattern": "/gallery.js",
      "path": "public/gallery.js"
    },
    {
      "pattern": "/layout.js",
      "path": "public/layout.js"
    },
    {
      "pattern": "/*",
      "path": "public/index.html"
    }
  ]
}
